 <querysource>
  <name>datos</name>
  <sql>SELECT strftime( "%d/%m/%Y", t1.fecha_hora ) AS fecha,
          t1.razon,
          t1.ingreso,
          t1.egreso,
          (select SUM(t2.ingreso)-SUM(t2.egreso) FROM movimiento_caja AS t2 WHERE t2.id_caja = &lt;? value( "id_caja" ) ?> AND t2.id_movimiento &lt;= t1.id_movimiento ) AS saldo
FROM movimiento_caja AS t1
WHERE t1.id_caja = &lt;? value( "id_caja", "filtro" ) ?></sql>
 </querysource>
 <querysource>
  <name>caja</name>
  <sql>SELECT nombre,
       saldo,
       strftime( "%d/%m/%Y", fecha_alta ) AS fecha_alta
FROM caja
WHERE id_caja = &lt;? value( "id_caja" ) ?></sql>
 </querysource>
