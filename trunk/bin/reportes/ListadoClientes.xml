 <querysource>
  <name>clientes</name>
  <sql>SELECT `id`,
       `razon_social`,
       `email`,
       `tel_fijo`,
       `tel_celular`,
        `calle` || ' ' || `numero` AS direccion
FROM `clientes` 
&lt;? if( exists("filtro") ?>
WHERE &lt;? value("filtro") ?>
&lt;? endif ?>
ORDER BY razon_social ASC</sql>
 </querysource>
