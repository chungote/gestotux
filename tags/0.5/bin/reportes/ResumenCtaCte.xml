 <querysource>
  <name>resumen</name>
  <sql>SELECT strftime( "%d/%m/%Y", t1.fecha ) AS fecha,
          t1.descripcion,
          CASE t1.tipo_op WHEN 0 THEN 'R'
		     WHEN 1 THEN 'F'
		     WHEN 2 THEN 'NC'
		     WHEN 3 THEN 'ND'
		     WHEN 4 THEN 'CS'
		     WHEN 5 THEN 'RCS'
		     WHEN 6 THEN 'FA'
	 END AS Tipo,
          t1.debe,
          t1.haber,
          (select SUM(t2.debe)-SUM(t2.haber) FROM item_ctacte AS t2 WHERE t2.id_ctacte = &lt;? value( "ctacte" ) ?> AND t2.id_item_ctacte &lt;= t1.id_item_ctacte ) AS saldo
FROM item_ctacte AS t1
WHERE t1.id_ctacte = &lt;? value( "ctacte", "filtro" ) ?></sql>
 </querysource>
 <querysource>
  <name>datos</name>
  <sql>SELECT cc.numero_cuenta,
       c.razon_social AS razon_social,
       strftime( "%d/%m/%Y", cc.fecha_alta ) AS fecha_alta,
       cc.saldo AS saldo,
       cc.limite AS limite
FROM ctacte AS cc, clientes AS c
WHERE cc.id_cliente = c.id AND cc.numero_cuenta = &lt;? value( "ctacte" ) ?></sql>
 </querysource>
